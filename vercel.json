{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      },
      {
        "src": "digit-recognizer/build/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/app.py"
      },
      {
        "src": "/static/(.*)",
        "dest": "/digit-recognizer/build/$1"
      },
      {
        "src": "/",
        "dest": "/digit-recognizer/build/index.html"
      }
    ]
  }
  